{
    "name": "align-by-regex",
    "displayName": "Align by RegEx",
    "description": "Aligns text by a regular expression.",
    "version": "0.0.1",
    "publisher": "janjoerke",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:align.by.regex"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "align.by.regex",
            "title": "Align by RegEx"
        }],
        "configuration": {
            "type": "object",
            "title": "Align by RegEx",
            "properties": {
                "align.by.regex.templates": {
                    "type": "object",
                    "description": "Stored RegEx templates."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}